   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated commands -->
      <package
            uri="http://www.eclipse.org/ecl/data/commands.ecore"
            class="org.eclipse.ecl.data.commands.CommandsPackage"
            genModel="model/commands.genmodel"/>
